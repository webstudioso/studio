{
    "name": "webstudio-app",
    "version": "0.1.19",
    "private": false,
    "dependencies": {
        "@auth0/auth0-spa-js": "^1.14.0",
        "@babel/preset-react": "^7.17.12",
        "@emotion/react": "^11.10.5",
        "@emotion/styled": "^11.10.5",
        "@hookform/resolvers": "1.3.5",
        "@mui/icons-material": "^5.6.1",
        "@mui/material": "^5.11.0",
        "@mui/system": "^5.0.3",
        "@tabler/icons": "^1.39.1",
        "apexcharts": "^3.26.1",
        "axios": "^1.3.6",
        "axios-mock-adapter": "^1.19.0",
        "chance": "^1.1.7",
        "date-fns": "^2.21.1",
        "framer-motion": "^4.1.13",
        "grapesjs": "^0.19.5",
        "grapesjs-plugin-forms": "^2.0.5",
        "grapesjs-script-editor": "^1.0.3",
        "jss": "^10.7.1",
        "jss-rtl": "^0.3.0",
        "jwt-decode": "^3.1.2",
        "magic-sdk": "^10.1.0",
        "material-icons": "^1.11.8",
        "moment": "^2.29.4",
        "prop-types": "^15.7.2",
        "react": "^18.2.0",
        "react-apexcharts": "^1.3.7",
        "react-device-detect": "^2.2.2",
        "react-dom": "^18.2.0",
        "react-draggable": "^4.4.5",
        "react-intl": "^6.4.2",
        "react-markdown": "^6.0.3",
        "react-perfect-scrollbar": "^1.5.8",
        "react-redux": "^7.1.0-rc.1",
        "react-responsive-carousel": "^3.2.23",
        "react-router-dom": "^6.0.0-beta.5",
        "react-scripts": "^5.0.1",
        "react-share": "^4.4.1",
        "redux": "^4.0.5",
        "redux-persist": "^6.0.0",
        "rehype-raw": "^6.1.1",
        "remark-gfm": "^1.0.0",
        "stylis-plugin-rtl": "^2.0.2",
        "uuid": "^8.3.2",
        "wsm-animations": "^0.0.2",
        "wsm-basic": "^0.0.1",
        "wsm-fonts": "^1.0.1",
        "wsm-form": "^0.0.41",
        "wsm-tailwind": "^1.0.5",
        "wsm-toast": "^1.0.14",
        "wsm-wallet-connect": "^0.0.51",
        "yup": "^0.32.9"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.20.2",
        "autoprefixer": "^10.4.14",
        "env-cmd": "^10.1.0",
        "eslint": "^8.23.0",
        "eslint-config-react-app": "^7.0.1",
        "husky": "^8.0.3",
        "jest-mock-axios": "^4.7.0-beta4",
        "lint-staged": "^13.1.0",
        "postcss": "^8.4.23",
        "sass": "^1.56.2",
        "tailwindcss": "^3.3.2"
    },
    "jest": {
        "setupFilesAfterEnv": [
            "<rootDir>/src/setupTests.js"
        ],
        "moduleDirectories": [
            "node_modules",
            "src"
        ],
        "coverageReporters": [
            "json",
            "text",
            "html",
            "lcov"
        ],
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!src/**/*/*.d.ts",
            "!src/**/*/Loadable.{js,jsx,ts,tsx}",
            "!src/**/*/types.ts",
            "!src/**/store.ts",
            "!src/index.tsx",
            "!src/serviceWorker.ts",
            "!<rootDir>/node_modules/",
            "!**/templates/**",
            "!**/template/**",
            "!src/__tests__/**",
            "!src/lib/assets/**"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 0,
                "functions": 0,
                "lines": 0,
                "statements": 0
            }
        },
        "testEnvironment": "jsdom",
        "transformIgnorePatterns": [
            "node_modules/(?!axios)"
        ]
    },
    "scripts": {
        "start": "export HTTPS=true&&SSL_CRT_FILE=cert.pem&&SSL_KEY_FILE=key.pem REACT_APP_VERSION=$npm_package_version env-cmd -f .env react-scripts start",
        "build:release": "REACT_APP_VERSION=$npm_package_version react-scripts build",
        "build": "env-cmd -f .env npm run build:release",
        "test": "jest --collectCoverage=true --passWithNoTests",
        "coverage": "jest --coverage --passWithNoTests",
        "eject": "react-scripts eject",
        "prepare": "husky install"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "lint-staged": {
        "*.js": [
            "eslint --cache --fix"
        ]
    },
    "resolutions": {
        "react": "^18.2.0"
    }
}
